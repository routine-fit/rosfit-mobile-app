{
  "name": "routinefit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn android:start.dev",
    "android:start.dev": "react-native run-android --mode=devDebug --appId=com.routinefit.dev",
    "android:start.prod": "react-native run-android --mode=prodDebug",
    "android:build.prod": "cd android/ && ./gradlew clean bundleProductionRelease",
    "ios": "yarn ios:start.dev",
    "ios:start.dev": "react-native run-ios --scheme RoutineFit-Dev --simulator=\"iPhone SE (3rd generation)\"",
    "ios:start.prod": "react-native run-ios --scheme routinefit-prod --simulator=\"iPhone SE (3rd generation)\"",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "prepare": "husky install",
    "pod:install": "cd ios && pod install && cd ..",
    "clear:modules": "rm -rf node_modules && yarn",
    "set:core": "yarn clear:modules && yarn pod:install && yarn start"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.4",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-firebase/app": "^21.0.0",
    "@react-native-firebase/auth": "^21.0.0",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "i18next": "^23.15.1",
    "lottie-react-native": "^7.0.0",
    "lucide-react-native": "^0.446.0",
    "polished": "^4.3.1",
    "react": "18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.2",
    "react-native": "0.75.3",
    "react-native-bouncy-checkbox": "^4.1.2",
    "react-native-config": "^1.5.3",
    "react-native-countdown-circle-timer": "^3.2.1",
    "react-native-date-picker": "^5.0.7",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-localize": "^3.2.1",
    "react-native-reanimated": "^3.15.3",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-redux": "^9.1.2",
    "styled-components": "^6.1.13",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^9.1.6",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "reactotron-core-client": "^2.9.4",
    "reactotron-react-native": "^5.1.8",
    "reactotron-redux": "^3.1.10",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
