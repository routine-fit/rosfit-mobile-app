{
  "name": "routinefit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn android:start.dev",
    "android:start.dev": "react-native run-android --mode=devDebug --appId=com.routinefit.dev",
    "android:start.prod": "react-native run-android --mode=prodDebug",
    "android:build.prod": "cd android/ && ./gradlew clean bundleProductionRelease",
    "ios": "yarn ios:start.dev",
    "ios:start.dev": "react-native run-ios --scheme RoutineFit-Dev --simulator=\"iPhone SE (3rd generation)\"",
    "ios:start.prod": "react-native run-ios --scheme routinefit-prod --simulator=\"iPhone SE (3rd generation)\"",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "prepare": "husky install",
    "pod:install": "cd ios && pod install && cd ..",
    "clear:modules": "rm -rf node_modules && yarn",
    "set:core": "yarn clear:modules && yarn pod:install && yarn start"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.54",
    "@gluestack-ui/config": "^1.1.18",
    "@gluestack-ui/themed": "^1.1.22",
    "@gorhom/bottom-sheet": "^4",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-firebase/app": "^19.2.2",
    "@react-native-firebase/auth": "^19.2.2",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.8",
    "date-fns": "^4.1.0",
    "i18next": "^23.7.16",
    "lottie-react-native": "^6.7.2",
    "lucide-react-native": "^0.308.0",
    "polished": "^4.3.1",
    "react": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^14.0.0",
    "react-native": "0.74.0",
    "react-native-config": "^1.5.1",
    "react-native-countdown-circle-timer": "^3.2.1",
    "react-native-date-picker": "^4.3.5",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-localize": "^3.0.5",
    "react-native-reanimated": "^3.10.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.2.0",
    "react-redux": "^8.1.3",
    "styled-components": "^6.1.9",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/runtime": "^7.24.5",
    "@react-native/babel-preset": "0.74.82",
    "@react-native/eslint-config": "0.74.82",
    "@react-native/metro-config": "0.74.82",
    "@react-native/typescript-config": "0.74.82",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-core-client": "^2.9.3",
    "reactotron-react-native": "^5.1.6",
    "reactotron-redux": "^3.1.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
